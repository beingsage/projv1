    function download(){
      var userAgent = navigator.userAgent || navigator.vendor || window.opera;
      if (/iPad|iPhone|iPod|Macintosh/.test(userAgent) && !window.MSStream) {
        window.location.assign("https://apps.apple.com/in/app/sportsbaazi-fantasy-app/id1438726846");
      } else {
        window.location.href = download_apk_url;     
      }
    }

 $(window).on('scroll', function (event) {
        var scroll = $(window).scrollTop();
        if (scroll < 200) {
            $(".sticky-install-button").removeClass("sticky");
        } else {
            $(".sticky-install-button").addClass("sticky");
        }
    });
</script>

